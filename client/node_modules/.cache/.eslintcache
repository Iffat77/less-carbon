ent/src/App.js",["63","64"],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/screens/Login.jsx",[],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/screens/Register.jsx",["65"],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/components/Nav.jsx",[],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/screens/Home.jsx",[],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/services/auth.js",["66"],[],"import axios from \"axios\";\nimport api from \"./apiConfig\";\n\n// Remider to update url before prod NOTE- fugure out api import issue\n\nconst baseUrl = \"http://localhost:8000/api/users\";\n\nconst authService = {\n  login: async (email, password) => {\n    const response = await axios.post(`${baseUrl}/login`, { email, password });\n    const token = response.data.token;\n\n    localStorage.setItem(\"token\", token);\n\n    return token;\n  },\n\n  register: async (name, email, password) => {\n    const response = await axios.post(`${baseUrl}`, { name, email, password });\n    return response.data.token;\n  },\n\n  logout: () => {\n    localStorage.removeItem(\"token\");\n  },\n\n  getUserData: async () => {\n    const token = localStorage.getItem(\"token\");\n\n    const headers = {\n      Authorization: `Bearer ${token}`,\n    };\n\n    try {\n      const response = await axios.get(`${baseUrl}/me`, { headers });\n      return response.data;\n    } catch (error) {\n      throw error;\n    }\n  },\n\n  isAuthenticated: () => {\n    const token = localStorage.getItem(\"token\");\n    return !!token; // Return true if token exists, false otherwise\n  },\n};\n\nexport default authService;\n","/Users/iffathossain/iffatdev/projects/less-carbon/client/src/services/apiConfig.js",[],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/screens/articles/ArticleInfo.jsx",["67"],[],"/Users/iffathossain/iffatdev/projects/less-carbon/client/src/services/atricles.js",[],[],"/Users/iffathossain/iff